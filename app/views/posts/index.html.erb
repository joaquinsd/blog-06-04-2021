<div class="container">
  <h1>Blog Posts</h1>
  <table class="table">
    <tr>
      <th>Title</th>
      <th>Posted In</th>
      <th>Image</th>
      <th>Content</th>
    </tr>
    <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.created_at %></td>
      <td><img src='<%= post.image_url %>' width="100"></td>
      <td><%= post.content %></td>
    </tr>
    <% end %>
  </table>
  <p>Create your own post <%= link_to "here", posts_dashboard_path %>!</p>
</div>